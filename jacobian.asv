function out = jacobian(S1, S2, S3, S4, x, y, z)
Dr = zeros(4, 4);
c = 299792.458;
Dr(1,1) = (x - 15600)/S1;
Dr(1,2) = (y - 7540)/S1;
Dr(1,3) = (z - 20140)/S1;
Dr(1,4) = c;
Dr(2,1) = (x - 18760)/S2;
Dr(2,2) = (y - 2750)/S2;
Dr(2,3) = (z - 18610)/S2;
Dr(2,4) = Dr(1,4);
Dr(3,1) = (x - 17610)/S3;
Dr(3,2) = (y - 14630)/S3;
Dr(3,3) = (z - 13480)/S3;
Dr(3,4) = Dr(2,4);
Dr(4,1) = (x - 19170)/S4;
Dr(4,2) = (y - 610)/S4;
Dr(4,3) = (z - 18390)/S4;
Dr(4,4) = Dr(3,4);

out = Dr;
end